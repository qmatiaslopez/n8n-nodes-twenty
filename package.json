{
  "name": "n8n-nodes-twenty-fork",
  "version": "2.1.0",
  "description": "n8n nodes for Twenty CRM with specialized nodes for better UX. Complete CRUD operations for People, Companies, Opportunities, and Notes. Full GraphQL API integration with smart field resolution and unified search capabilities.",
  "keywords": [
    "n8n-community-node-package",
    "twenty-crm",
    "crm",
    "graphql",
    "people",
    "companies",
    "opportunities",
    "notes",
    "sales-pipeline",
    "twenty-fork",
    "twenty-v1.4.0",
    "community-fork",
    "unified-search",
    "smart-fields",
    "custom-fields",
    "field-resolution",
    "automation",
    "workflow",
    "specialized-nodes",
    "modular"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/qmatiaslopez/n8n-nodes-twenty",
  "author": {
    "name": "Matias Lopez",
    "url": "https://github.com/qmatiaslopez"
  },
  "contributors": [
    {
      "name": "S Hodgson",
      "email": "steven@hodgolabs.com",
      "url": "https://github.com/shodgson/n8n-nodes-twenty"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/qmatiaslopez/n8n-nodes-twenty"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TwentyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Twenty/Twenty.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@devlikeapro/n8n-openapi-node": "^0.1.4"
  }
}
